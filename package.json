  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "node build/dev-server.js",
    "build": "node build/build.js",
    "build:preview": "cross-env npm_config_preview=true node build/build.js",
    "faker": "node ./BACKEND-fake/faker-server",
    "start-both": "start npm run faker && start npm run start",
    "json-products": "./node_modules/.bin/json-server ./BACKEND-fake/faker-server/faker/generate_products.js",
    "json-users": "json-server ./BACKEND-fake/faker-server/faker/generate_users.js",
    "json-people": "json-server ./BACKEND-fake/faker-server/faker/generate_people.js",
    "svgo": "svgo -f src/components/SvgIcon/svg --config=src/components/SvgIcon/svgo.yml",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run e2e",
    "lint": "eslint --ext .js,.vue src test/e2e/specs"
  },
  "keywords": [
    "vue",
    "adminLTE",
    "element-ui",
    "dashboard",
    "mirage",
    "admin-panel",
    "admin-template"
  ],
